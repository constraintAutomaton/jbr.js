{
  "name": "jbr-solidbench-shapetree",
  "version": "0.0.2",
  "description": "SolidBench experiment handler for JBR",
  "main": "index.js",
  "typings": "index",
  "lsd:module": true,
  "files": [
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "lib/templates",
    "index.d.ts",
    "index.ts",
    "components"
  ],
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "../../tsconfig.json"
      }
    },
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true
  },
  "scripts": {
    "test": "node \"../../node_modules/jest/bin/jest.js\" ${1}",
    "test-watch": "node \"../../node_modules/jest/bin/jest.js\" ${1} --watch",
    "build": "node \"../../node_modules/typescript/bin/tsc\"",
    "validate": "npm ls"
  },
  "devDependencies": {
    "jbr": "^3.0.2"
  },
  "peerDependencies": {
    "jbr": ">=2.0.0"
  },
  "dependencies": {
    "@types/fs-extra": "^9.0.11",
    "@types/yargs": "^17.0.0",
    "fs-extra": "^10.0.0",
    "solidbench-shapetree": "^1.7.6",
    "sparql-benchmark-runner": "^2.9.4"
  }
}
